<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Control</servlet-name>
        <servlet-class>controlador.Control</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>sube</servlet-name>
        <servlet-class>controlador.sube</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>grafico1</servlet-name>
        <servlet-class>controlador.grafico1</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>control2</servlet-name>
        <servlet-class>controlador.control2</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Sube</servlet-name>
        <servlet-class>controlador.Sube</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Control</servlet-name>
        <url-pattern>/Control</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>sube</servlet-name>
        <url-pattern>/sube</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>grafico1</servlet-name>
        <url-pattern>/grafico1</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>control2</servlet-name>
        <url-pattern>/control2</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Sube</servlet-name>
        <url-pattern>/Sube</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
